    "tsc": "tsc",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint . --ext .ts"
  },
  "author": "Isaac Wahba",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
